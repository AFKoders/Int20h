<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape>
            <corners android:radius="5dp" />
            <solid android:color="#FFF" />
        </shape>
    </item>

    <item
        android:id="@android:id/progress"
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        android:top="3dp">
        <shape>
            <corners android:radius="4dp" />
            <solid android:color="#00C48C" />
        </shape>
    </item>

</layer-list>